git submodule status 
ls -la .git
cat .gitmodules 
git remote -v
git remote add origin https://github.com/juanvolpe/testfx2.git 

# First, let's check the current status
git status

# Let's verify we're in the correct branch
git branch

# Initialize the repository if not already done
git init

# Add the remote repository again to ensure correct connection
git remote remove origin
git remote add origin https://github.com/juanvolpe/testfx2.git

# Stage all changes
git add .

# Create a new commit with a test message
git commit -m "test: Initial commit to verify repository connection
- Testing GitHub connection
- Adding Kunai animations
- Adding scroll effects
- Adding section animations"

# Set the main branch and push
git branch -M main
git push -u origin main --force 